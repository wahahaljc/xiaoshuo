<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mybatisplus.entity.Menu">
        <id column="id" property="id" />
        <result column="patent_id" property="patentId" />
        <result column="title" property="title" />
        <result column="icon" property="icon" />
        <result column="path" property="path" />
        <result column="authority" property="authority" />
        <result column="menu_Type" property="menuType" />
        <result column="sortNumber" property="sortNumber" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, patent_id, title, icon, path, authority, menu_Type, sortNumber
    </sql>

</mapper>
