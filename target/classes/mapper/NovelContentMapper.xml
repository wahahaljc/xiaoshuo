<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mapper.NovelContentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mybatisplus.entity.NovelContent">
        <id column="id" property="id" />
        <result column="n_id" property="nId" />
        <result column="content" property="content" />
        <result column="chaptersandsections" property="chaptersandsections" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, n_id, content,chaptersandsections
    </sql>

</mapper>
